{
  "manifest_version": 3,
  "name": "LinkedOut",
  "version": "1.0",
  "description": "Removes Home button and redirects Feed to Profile.",
  "author": "Piotrr0",
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["src/main.js"],
      "run_at": "document_end"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "linkedout@piotrr0.github",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}